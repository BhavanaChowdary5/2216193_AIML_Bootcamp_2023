        "#11. Implement Bitwise operators with examples. \n",
        "# & (bitwise AND)\n",
        "x = 5  # binary: 101\n",
        "y = 3  # binary: 011\n",
        "result = x & y\n",
        "print(result)\n",
        "# | (bitwise OR)\n",
        "x = 5  # binary: 101\n",
        "y = 3  # binary: 011\n",
        "result = x | y\n",
        "print(result)\n",
        "# ^ (bitwise XOR) \n",
        "x = 5  # binary: 101\n",
        "y = 3  # binary: 011\n",
        "result = x ^ y\n",
        "print(result)\n",
        "# ~ (bitwise NOT)\n",
        "x = 5  # binary: 101\n",
        "result = ~x\n",
        "print(result)\n",
        "# << (left shift)\n",
        "x = 5  # binary: 101\n",
        "result = x << 1\n",
        "print(result)\n",
        "# >> (right shift)\n",
        "x = 5  # binary: 101\n",
        "result = x >> 1\n",
        "print(result)\n",
        "     "
